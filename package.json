{
  "name": "spotify-sync",
  "author": "LuisEnMarroquin <luis@marroquin.dev>",
  "description": "Sync your Spotify's playback history every hour",
  "private": true,
  "scripts": {
    "start": "tsl dev src/server.ts 'npm run client'",
    "client": "tsl client src/scripts.ts public",
    "server2": "tsl client src/server.ts dist",
    "server": "tsl server src/server.ts dist"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "cron": "^1.8.2",
    "dotenv": "^8.1.0",
    "express": "^4.16.0",
    "handlebars": "^4.7.7",
    "mongoose": "^5.11.17",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.0.9",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/cron": "^1.7.2",
    "@types/jquery": "^3.5.5",
    "@types/request": "^2.48.5"
  },
  "nodemonConfig": {
    "ext": "html,css,js,ts,json",
    "ignore": [
      "dist/",
      "public/*.js"
    ]
  }
}
